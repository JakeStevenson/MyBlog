                ul.nav.navbar-nav
                    li.active
                        a(href='/') Blog
                    li
                        a(href='/about') About
                    li
                        a(href='/contact') Contact
//
          /.nav-collapse
    .container
        .starter-template
            h1 
                block title
                      = locals.name
            p.lead
                = locals.description
        .bs-example
            block body
            block content
//
       /.container
